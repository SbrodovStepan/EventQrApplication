<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="eqa">
        <createTable tableName="EQA_EVENT_REQUEST">
            <column name="ID" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="PK_EQA_EVENT_REQUEST"/>
            </column>
            <column name="EVENT_CODE" type="VARCHAR(255)"/>
            <column name="EVENT_NAME" type="VARCHAR(255)"/>
            <column name="EVENT_HALL" type="VARCHAR(255)"/>
            <column name="EVENT_DATE" type="VARCHAR(255)"/>
            <column name="TIME_START" type="VARCHAR(255)"/>
            <column name="TIME_END" type="VARCHAR(255)"/>
            <column name="LAST_NAME" type="VARCHAR(255)"/>
            <column name="FIRST_NAME" type="VARCHAR(255)"/>
            <column name="MIDDLE_NAME" type="VARCHAR(255)"/>
            <column name="ORGANIZATION" type="VARCHAR(255)"/>
            <column name="QR_CODE" type="BLOB"/>
        </createTable>
    </changeSet>
</databaseChangeLog>